<shapes name="mxgraph.plumber.valves">
<shape aspect="variable" h="40" w="40" name="threewayvalve" strokewidth="inherit" >
    <foreground>
        <path>
            <move x="0" y="10"/>
            <line x="40" y="30"/>
            <line x="40" y="10"/>
            <line x="0" y="30"/>
            <line x="0" y="10"/>
            <move x="20" y="20"/>
            <line x="10" y="40"/>
            <line x="30" y="40"/>
            <line x="20" y="20"/>
            <close/>
        </path>
        <fillstroke/>
    </foreground>
</shape>
<shape aspect="variable" h="40" w="40" name="valveaddon" strokewidth="inherit" >
    <foreground>
        <path>
            <move x="0" y="10"/>
            <line x="20" y="20"/>
            <line x="0" y="30"/>
            <line x="0" y="10"/>
            <close/>
        </path>
        <fillstroke/>
    </foreground>
</shape>
<shape aspect="variable" h="40" w="40" name="valve" strokewidth="inherit" >
    <foreground>
        <path>
            <move x="0" y="10"/>
            <line x="40" y="30"/>
            <line x="40" y="10"/>
            <line x="0" y="30"/>
            <line x="0" y="10"/>
            <close/>
        </path>
        <fillstroke/>
    </foreground>
</shape>
</shapes>